{
  "name": "@keplr-wallet/mobile",
  "version": "0.12.22-rc.0",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "clean": "sh ./scripts/clean.sh",
    "android": "yarn build:provider && react-native run-android",
    "android:production": "yarn build:provider && react-native run-android --mode Release",
    "android:connect": "adb reverse tcp:8081 tcp:8081",
    "build:provider": "tsc --project tsconfig.provider.json && browserify -p tinyify build/injected/index.js -o build/injected/injected-provider.bundle.js && cp -rf build/injected/injected-provider.bundle.js android/app/src/main/assets/.",
    "ios": "yarn build:provider && npx pod-install && react-native run-ios",
    "ios:production": "yarn build:provider && npx pod-install && react-native run-ios --mode Release",
    "lint-test": "eslint . --ext .js,.jsx,.ts,.tsx && prettier --check .",
    "lint-fix": "eslint . --fix .js,.jsx,.ts,.tsx && prettier --write .",
    "typecheck": "tsc --noEmit",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "test": "jest",
    "force-export-ble": "sh ./scripts/force-export-ble-manager-from-ledger.sh"
  },
  "dependencies": {
    "@formatjs/intl-getcanonicallocales": "^2.2.1",
    "@formatjs/intl-locale": "^3.3.4",
    "@formatjs/intl-numberformat": "^8.7.2",
    "@formatjs/intl-pluralrules": "^5.2.6",
    "@formatjs/intl-relativetimeformat": "^11.2.6",
    "@gorhom/bottom-sheet": "^4.5.0",
    "@ledgerhq/hw-app-eth": "^6.29.3",
    "@ledgerhq/react-native-hw-transport-ble": "=6.20.0",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/blur": "^4.3.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.1.7",
    "buffer": "^6.0.3",
    "expo": "^49.0.6",
    "expo-clipboard": "~4.3.1",
    "expo-crypto": "~12.4.1",
    "expo-standard-web-crypto": "^1.6.0",
    "expo-web-browser": "~12.3.2",
    "http-browserify": "^1.7.0",
    "https-browserify": "^1.0.0",
    "lottie-react-native": "^6.3.1",
    "mobx": "^6.10.0",
    "mobx-react-lite": "^3.4.3",
    "mobx-utils": "^6.0.8",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "react": "18.2.0",
    "react-hook-form": "^7.45.4",
    "react-intl": "^6.2.8",
    "react-native": "0.72.3",
    "react-native-ble-plx": "^2.0.3",
    "react-native-device-info": "^10.11.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keychain": "^8.1.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-permissions": "^3.10.1",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-reanimated": "^3.4.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-svg": "^13.13.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-webview": "^13.6.2",
    "setimmediate": "^1.0.5",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "text-encoding": "^0.7.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.9",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react-test-renderer": "^18.0.0",
    "@types/setimmediate": "^1",
    "@types/text-encoding": "^0",
    "babel-jest": "^29.2.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "browserify": "^17.0.0",
    "metro-react-native-babel-preset": "0.76.7",
    "pod-install": "^0.1.39",
    "react-test-renderer": "18.2.0",
    "tinyify": "^4.0.0"
  },
  "engines": {
    "node": ">=16"
  }
}
