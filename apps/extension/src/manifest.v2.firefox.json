{
  "applications": {
    "gecko": {
      "id": "keplr-extension@keplr.app",
      "strict_min_version": "89.0"
    }
  },
  "icons": {
    "16": "assets/icon-beta-16.png",
    "48": "assets/icon-beta-48.png",
    "128": "assets/icon-beta-128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["injectedScript.bundle.js"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["assets/Inter-SemiBold.ttf"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["assets/icon-48.png"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["assets/megaphone.svg"],
      "matches": ["<all_urls>"]
    },
    {
      "resources": ["assets/locked-keplr-logo-128.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
